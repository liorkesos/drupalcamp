@charset "utf-8";
/**
 * Doil: A sub-theme of Tendu  - A CSS theme for developers
 * Made for http://Drupal.org.il
 * $Id: style_subtheme.css,v 1.1.2.5 2009/01/24 11:00:48 tombigel Exp $
 * Author: Tom Bigelajzen - http://tombigel.com
 */
body{color:#333;background:#fff url(images/bg_body.gif) repeat-x 0 0;padding:0 12px;}

/**
 * Override page width of base Tendu theme
 */
#page{min-width:950px;max-width:1250px;width:990px;}

/**
 * Override sidebars widths of the base Tendu theme.
 */
#sidebar-first{width:150px;}
#sidebar-second{width:315px;}

.with-sidebar-first #content{margin-left:150px;}/*LTR*/
.with-sidebar-second #content{margin-right:315px;}/*LTR*/

/*Basic Typography*/
h1{font-size:1.833em;line-height:1.333em;font-weight:bold;}
h2{font-size:1.5em;line-height:1.333em;font-weight:bold;}
h3{font-size:1.333em;line-height:1.333em;font-weight:bold;}
h4{font-size:1.166em;line-height:1.333em;font-weight:bold;}
h5{font-size:1.083em;line-height:1.333em;font-weight:bold;}
h6{font-size:1em;line-height:1.333em;font-weight:bold;}
p{padding-bottom:.5em;}
.meta{font-size:1em;padding-bottom:.5em;color:#666;}

/*Tables*/
table{margin-top:10px;}
tr.even{background:#fcfdfe;}
td,
th{padding:.2em .5em;}

/*Lists*/
dt{font-style:italic;}
dd{padding:.5em;}
ul,
ol{padding-left:1em;}/*LTR*/
ul li{margin-left:1.5em;list-style:disc;line-height:1.5em;}/*LTR*/
ol li{margin-left:1.5em;list-style:decimal;line-height:1.5em;}/*LTR*/
.item-list ul li{padding:3px 0;}
ul.links li{margin:0;}

/* Form Elements  */
fieldset{padding:.5em .8em;margin:0 0 1em;border:1px solid #dddff0;background:#fdfeff;}
html.js fieldset.collapsed{background:none;}
legend{font-weight:bold;font-size:1.166em;padding:0 .3em}
input, select, textarea{font-family:Arial, Helvetica, sans-serif;font-weight:normal;line-height:1em;font-size:1em;}
textarea{line-height:1.2em;}
button{font-family:Arial, Helvetica, sans-serif;font-weight:bold;line-height:1em;font-size:13px}
button{cursor:pointer;}
.form-radio,
.form-checkbox{vertical-align:middle;}/* TODO: Check how this works site wide */

/* Links */
a{color:#0073bb;text-decoration:none;font-weight:bold;}
a:visited{color:#1f7bb3;}
a:hover, a:focus{text-decoration:underline;}
a:active,
a.active:hover,
a.active:visited{color:#264463;}

/* Header and Navagation */
#header{position:relative;}
#header-content{background:#1984c7 url(images/bg_header.gif) repeat-x 0 100%;color:#fff;border:solid #0c71ae;border-width: 0 1px 1px 1px;}

#header a{color:#fff;}
#header a:visited{color:#fafafa;}

/*Header Blocks*/
#header-blocks{float:right;width:370px;padding:12px;}/*LTR*/
#header-blocks .block{margin-bottom:10px;}
#header-blocks .block-last{margin-bottom:0;}
/*Theme Language Switcher in header blocks*/
#header-blocks #language-switcher{margin-bottom:10px;}
#header-blocks #language-switcher ul{text-align:right;padding-left:0;overflow:hidden;height:1%;font-size:.916em;}/*LTR*/
#header-blocks #language-switcher li{margin:0;padding:0 .5em;list-style:none;display:inline;}
#header-blocks #language-switcher a{font-weight:bold;}
/*Theme Search Box in header blocks*/
#header-blocks #search-box{margin-bottom:10px;}
#header-blocks #search-box label{display:none}
#header-blocks #search-box input{vertical-align:middle}
#header-blocks #search-box .form-text{width:290px;border:1px solid #7fafcc;font-size:1.333em;padding:3px;}
#header-blocks #search-box .form-submit{font-size:1.2em;height:1.95em;width:69px;padding:3px 0;border:1px solid #495f6d;background:#afafaf url(images/bg_btn_search.gif) repeat-x 0 0;}
#header-blocks #search-box .container-inline{white-space:nowrap;}

/*Site details*/
#site-details{float:left;padding:12px}/*LTR*/
#site-logo{float:left;padding:5px 0 0 10px}/*LTR*/ /*IE6*/
#site-name{font-size:2.5em;line-height:1.333em;font-weight:bold;}
#site-slogan{font-size:1.5em;line-height:1.333em;}

/*Mission*/
#mission{clear:left;margin-right:395px;padding:0 12px 12px 24px;font-size:1.333em;line-height:normal}/*LTR*/
#mission a, 
#mission a:hover, 
#mission a:visited{font-weight:bold;color:#fff;text-decoration:underline;}

/*Navagation (Primary links)*/
#main-nav{position:relative;top: -1px;}
#main-nav ul.menu{overflow:hidden;padding:0;}
#main-nav ul.menu li{float:left;overflow:hidden;display:inline;padding:0;margin-left:1em;list-style:none;}/*LTR*/
#main-nav ul.menu li a{display:block;height:1.7em;float:left;color:#23243b;padding:.3em 1em 0;font-weight:bold;font-size:1.083em;}/*LTR*/
#main-nav ul.menu li a:visited{color:#23243b;}
#main-nav ul.menu li a.active,
#main-nav ul.menu li a.active:visited{color:#fff;background:#64b8ea url(images/bg_tab_active.gif) repeat-x 0 0;border:solid #0c71ae;border-width:0 1px 1px 1px;}

/* Front page header overrides */
.front #header{padding-top:2.3em;}
.front #header-content{background-position:0 0;background-color:#2898da;border-width:1px;}
.front #main-nav{position:absolute;top:.3em;padding-top:1px;}
.front #main-nav ul li.first{margin:0;}
.front #main-nav ul.menu li a{height:1.6em;padding-top:.2em;}
.front #main-nav ul.menu li a.active,
.front #main-nav ul.menu li a.active:visited{background:#1a85c8 url(images/bg_tab_home_active.gif) repeat-x 0 0;border-width:1px 1px 0 1px;}

/* Page info */
.breadcrumb{padding:0;}
#content-title{margin-bottom:0;margin-top:0;}
#content-title .title{padding:0 12px}
#content-info{padding:6px 12px;margin-bottom:0;}
div.messages{padding:12px;margin-bottom:10px;}
.status{background:#f0fdf0;border:1px solid #e0ede0;}

/* Blocks in sidebars */
.sidebar-region .block{margin:10px 0;padding:6px;}
.sidebar-region #nav a{font-weight:bold;font-size:1.083em;}
.sidebar-region .block-first{margin-top:0;}

/* Blocks nodes and 
views in content areas. */
.extra-region .block,
.content-region div.block,/* Using "div.block" because /admin/build/blocks uses td.block */
.content-region .node,
.content-region .node-form,
.content-region .view,
.content-region .profile,
.content-region .node-type-list,
.page-admin #default-content{padding:12px;margin-bottom:10px;margin-top:0;}

.node{padding-bottom:.5em;border-bottom:1px solid #e4e4e4;}
.page-type-node .node{padding-bottom:0;border-bottom:none;}

#content{margin-top:10px;}
#before-content{margin-top:10px;}
#before-content .block-last{margin-bottom:0;}

.block-header{width:100%;overflow:hidden;}
.block-header h2{float:right;}
.block-header .links{display:block;text-align:left;font-size:1.5em;}
/* Style blocks with Block Class (or Block Theme):*/
.block-green-light {padding:12px 0 0 0 !important;border:1px solid #bfd3bd;background:#ecfbeb url(images/bg_green_light_top.gif) repeat-x 0 0 !important; overflow:hidden}
.block-green-light h2{color:#000;padding:0 12px;}
.block-green-light .content{color:#333;padding:12px;background: url(images/bg_green_light_bottom.gif) repeat-x 0 100%;}

/*Tweak green block in user blocks*/
#user-blocks .block-green-light{border-color:#697668;}
#user-blocks .block-green-light h2{color:#58943f;}
#user-blocks .block-green-light a{color:#0073bb;text-decoration:none;}
#user-blocks .block-green-light a:hover{text-decoration:underline;}
#user-blocks .block-green-light a:visited{color:#1f7bb3;}

.block-blue-light {padding:12px 0 0 0 !important;border:1px solid #b8d4e4;background:#d5ebf8 url(images/bg_blue_light_top.gif) repeat-x 0 0 !important; overflow:hidden}
.block-blue-light h2{color:#000;padding:0 12px;}
.block-blue-light .content{color:#333;padding:12px;background: url(images/bg_blue_light_bottom.gif) repeat-x 0 100%}

/* Sticky nodes on front page */
.front .sticky{overflow:hidden;padding:0;border:1px solid #deecf5;background:#f1f7fb url(images/bg_sticky_top.gif) repeat-x 0 0;}
.front .sticky .node-bottom{background:url(images/bg_sticky_bottom.gif) repeat-x 0 100%;margin:0;padding:0 12px 12px;}
.front .sticky h2{padding:12px 12px 0;}
.front .sticky .content,
.front .sticky .meta{padding:0 12px;}

/* Remove redundent styling from blocks in blocks, nodes and views */
.content-region .view div.block,
.content-region .node div.block,
div.block div.block{padding:0;background:0;}

/* Forums */
#forum table{width:95%;}

/* Tabs */
div.tabs{background:#1984c7 url(images/bg_header.gif) repeat-x 0 0;color:#fff;border:1px solid #0c71ae;padding:.5em 1em;}
ul.primary {border:none;margin:6px 0;}
ul.primary li{margin:0;line-height:normal;white-space:nowrap;}
ul.primary li a,
ul.primary li a:visited {background:none;border:none;padding:6px .6em;font-weight:bold;font-size:1.083em;color:#fff;}/*LTR*/
ul.primary li a:hover {text-decoration:underline;background:none;}
ul.primary li.active a,
ul.primary li.active a:hover,
ul.primary li.active a:visited{text-decoration:none;font-weight:bold;color:#0c71ae;background:#fff;border:none;}

ul.secondary {border:none;padding:.5em 0;background:#fff;margin:-4px 0 0 0;}
ul.secondary li{margin:0;line-height:normal;padding:0;border:none;white-space:nowrap;}
ul.secondary li a,
ul.secondary li a:visited {background:none;border:none;padding:0 .6em;color:#0073bb}
ul.secondary li a:hover {text-decoration:underline;background:none;}
ul.secondary li.active a,
ul.secondary li.active a:hover,
ul.secondary li.active a:visited{text-decoration:none;font-weight:bold;color:#206291;background:none;border:none;}

/* Footer */
#footer{font-size:.916em;border-top:1px solid #0074bb;}
#footer-message,
#footer-blocks{padding:6px 12px}
#footer p{padding:0;}
#footer ul li { list-style:none }

/*Doil Sprint*********************************************************************/
/* Increase Footer size */
#footer{height:10em}
.footer-spacer{padding-bottom:10em}
#footer-message, #footer-blocks{float:right; margin-left:2em;clear:left}

