/*User block*/
#sidebar-second .block .picture { display:inline }

/*Selected Sites*/
#before-content{padding:10px 0 0;border-bottom:1px solid #999;}
#before-content .block{padding:0}
#before-content .view{padding:0 10px;}
#before-content h2{padding:0 10px 8px;}
#before-content .block-promoted-israeli .view-content{padding-left:15px;}
#before-content .view-footer{text-align:left;width:100%;padding-top:8px;}
#before-content .view-footer a{font-size:1.25em;}
#before-content a{font-weight:bold}
.block-promoted-israeli .views-field-field-screenshots-fid img,
.block-promoted-world .views-field-field-screenshots-fid img{float:right;width:55px;height:55px;padding:1px;border:1px solid #666;margin-left:7px;}
.block-promoted-israeli .views-row-first .views-field-field-screenshots-fid img{width:115px;height:115px;}
.block-promoted-israeli{width:70%;float:right;}
.block-promoted-world{margin-right:70%}
.block-promoted-israeli .views-row-odd,
.block-promoted-israeli .views-row-even{margin-right:50%;overflow:hidden;margin-bottom:10px;min-height:59px;}
.block-promoted-israeli .views-row-first{float:right;margin-right:0;width:50%;min-height:119px;font-size:1.166em;font-weight:bold}
.block-promoted-world .views-row-odd,
.block-promoted-world .views-row-even{width:100%;overflow:hidden;margin-bottom:10px;min-height:59px;}

/*Node*/
.meta-next-to-title{float:left;padding:10px 3px;}
.node-bottom{text-align:left;float:left;padding:.4em 0;}
.node .terms{display:block;padding:.4em 0;}

/*Sidebar agreggation */
.view-doil-latest-content .views-field-name a{font-weight:normal;color:#333;}

/*welcome block*/
#block-block-23 h2{text-align:center;font-size:1.666em}
#block-block-23 #drupal_download{display:block;width:171px;padding-right:25px;height:45px;line-height:43px;white-space:nowrap;font-size:1.25em;color:#fff;text-align:center;background:url(images/btn_download.gif) no-repeat  0 0;margin:0 auto;}
#block-block-23 #more_drupal_versions{font-size:.83em;text-align:center;display:block;}

.magic_tabs {width:100%; overflow:hidden;}
.magic_tabs .tabs{width:100%;overflow:hidden;padding:0;margin:0;}
.magic_tabs .tabs li{list-style-type:none;margin:0;float:right;}
#user-blocks .magic_tabs .tabs li a{padding:4px 8px;display:block;display:inline-block;color:#063049}
#user-blocks .magic_tabs .tabs li a.selected{border:1px solid #C1CFC0;background:#fff;}
.magic_tabs table, 
.magic_tabs thead,
.magic_tabs tbody,
.magic_tabs th,
.magic_tabs tr,
.magic_tabs td{border:none;background:none;}
.magic_tabs{}
.magic_tabs{}

/*Comments*/

.comment-published {margin:6px 0;padding:12px 0 0 0 !important;border:1px solid #b8d4e4;background:#d5ebf8 url(images/bg_blue_light_top.gif) repeat-x 0 0;overflow:hidden}
.comment-published h3{color:#000;padding:0 12px;}
.comment-published .picture{padding:0 12px;float:left}
.comment-published .submitted{color:#666;padding:0 12px;}
.comment-published .content{color:#333;padding:12px 12px 6px;background: url(images/bg_blue_light_bottom.gif) repeat-x 0 100%}
.comment blockquote{padding:.5em;margin:.5em 2em .5em .5em;font-style:italic;color:#555;}
/*********************************************************************************/
/* Tweak Admin panel */
div.admin .right,
div.admin .left{margin:0;}

/* Tweak Views Admin panel */
a.views-button-remove{top:0;}
#edit-views-apply{margin-top:1.3em;}

/* Tweak Blocks Admin panel */
table#blocks{width:100%;}
#blocks td{vertical-align:middle;}
#blocks .block{padding:0;margin:0;}

/*Tweak Update Admin panel*/
.version-status .icon img{vertical-align:middle}

/**
 * Some fun with rounded corners.
 * Mozilla and Webkit support the "border-radius" property, so why not take advantage of it?
 * 
 * "-moz-border-radius" is for Mozilla browsers
 * "-webkit-border--top/bottom-left/right-radius" is for Webkit browsers 
 * "border-radius" is for a time where this property will get into the standard implementation (Has no effect in current versions)
 */
.block .content,
.sticky ul.links,
#header-content,
#header{-moz-border-radius:0 0 8px 8px;-webkit-border-bottom-left-radius: 8px;-webkit-border-bottom-right-radius: 8px;border-radius:0 0 8px 8px;}

.magic_tabs .tabs a,
div.tabs,
ul.secondary,
ul.primary li.active a,
#search-box input,
div.messages,
fieldset{-moz-border-radius:4px;-webkit-border-radius:4px;border-radius:4px;}

.comment-published,
#main-nav,
.front #header-content,
.sticky,
#content-area,
#content-title,
#content-info,
#default-content,
#mission,
#header-blocks .block,
.sidebar-region .block,
.extra-region   .block,
.content-region .block,
.content-region .node,
.content-region .node-form,
.content-region .view,
.content-region .profile{-moz-border-radius:8px;-webkit-border-radius:8px;border-radius:8px;}

.front #main-nav ul.menu li a.active{-moz-border-radius:4px 4px 0 0;-webkit-border-top-left-radius:4px;-webkit-border-top-right-radius:4px;-webkit-border-bottom-left-radius: 0;-webkit-border-bottom-right-radius: 0;border-radius:4px 4px 0 0;}
#main-nav ul.menu li a.active{-moz-border-radius:0 0 4px 4px;-webkit-border-bottom-left-radius: 4px;-webkit-border-bottom-right-radius: 4px;border-radius:0 0 4px 4px;}

/*Resets*/
.front #header-content,
ul.secondary{-moz-border-radius-topleft:0;-webkit-border-top-left-radius:0;border-top-left-radius:0;}
#blocks .block{-moz-border-radius:0;-webkit-border-radius:0;border-radius:0;background:none;}

/* Image thumbnails */
.field-field-screenshots .field-item {margin:1em; float:right;}
.node-bottom{clear:both}


